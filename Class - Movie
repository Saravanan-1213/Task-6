class Movie{
    constructor(title,studio,rating){
        this.title = title;
        this.studio = studio;
        this.rating = rating;
        
    }
}

var M1 = new Movie("Casino Royale","Eon Productions","PG13")

console.log(M1.studio);
console.log(M1.studio);
console.log(M1.rating);

 